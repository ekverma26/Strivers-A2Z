class Solution{
    static int evenlyDivides(int N){
        // code here
        int num=N;
        int d=0;
        while(num!=0)
        {
            int x=num%10;
            num=num/10;
            if(x>0)
            {
            if(N%x==0)
            {d++;}
            }
            else
            continue;
            
            
        }
        return d;
    }
}
