public class Solution {
    public static int countDigits(int n){
        // Write your code here.
        int c=0;
        int a=n;
        while(n>0){
            int r= n%10;
            if(r!=0){
            if(a%r==0)
                c++;
            }
            n=n/10;
            
        }
        return c;
    }
}
